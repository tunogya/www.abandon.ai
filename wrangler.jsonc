{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "abandon-ai-api",
	"compatibility_date": "2026-02-01",
	"main": "./workers/api/index.ts",
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "abandon_ai_db",
			"database_name": "abandon-ai-db",
			"database_id": "426355b2-ae29-4438-96f3-03a879abd5f8"
		},
	],
	"routes": [
		{
			"pattern": "api.abandon.ai/*",
			"zone_name": "abandon.ai"
		}
	]
